var hak ,can;
var tahmin,sayac=0;
var sayi = Math.random((Math.random()*10)+1);
can = Number(prompt('kaç kerede biliceksiniz?'))
hak=can;
while(hak>0){
    hak--;
    sayac++;
    tahmin = Number(prompt('bir sayı giriniz'))

     if(sayi==tahmin){
         console.log(`Tebrik ederiz ${sayac} defa bildiniz`)


         console.log(`puan : ${100-(100/can)*(sayac-1)}`)


         break;       
     }else if(sayi>tahmin){
         console.log('yukarı')
     }else{
         console.log('aşağı')
     }

     if(hak==0){
         console.log('hakkınız bitti :'+sayi)
     }
}

//Hata olduğunu düşünüyorum videoda tahmin edeceğimiz sayıyı veriyor ama bende göstermiyor 
